<Window x:Class="DesktopApp.CheckOutWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DesktopApp"
        mc:Ignorable="d"
        Title="CheckOutWindow" Height="450" Width="800">
    <Grid>
        <TextBox x:Name="CustomerNameTextBox" HorizontalAlignment="Left" Height="23" Margin="29,42,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="CheckOutCustomer" Content="Check Out" HorizontalAlignment="Left" Margin="299,42,0,0" VerticalAlignment="Top" Width="75" Click="CheckOutCustomer_Click"/>
        <Button x:Name="CancelCheckOutButton" Content="Cancel" HorizontalAlignment="Left" Margin="344,95,0,0" VerticalAlignment="Top" Width="75" Click="CancelCheckOutButton_Click"/>

    </Grid>
</Window>
